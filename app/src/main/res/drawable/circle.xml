<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item>
    <shape>
        <padding
            android:bottom="1dp"
            android:left="1dp"
            android:right="1dp"
            android:top="1dp" />

        <solid android:color="#00CCCCCC" />

        <corners android:radius="30dp" />
    </shape>
</item>

<item
    android:left="0dp"
    android:right="1dp"
    android:top="0dp"
    android:bottom="1dp">
    <shape android:shape="oval">
        <gradient
            android:startColor="#07891F"
            android:endColor="#136126"

            android:gradientRadius="100dp"
            android:type="radial"
            />
        <size
            android:width="120dp"
            android:height="120dp"/>
    </shape>
</item>
</layer-list>